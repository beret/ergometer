{
  "manifest_version": 2,
  "name": "Ergometer",
  "version": "5.1.3",
  "permissions": [
    "background",
    "idle",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "page": "extension.html"
  },
  "browser_action": {
    "default_popup": "details.html"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
